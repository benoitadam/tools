  "scripts": {
    "build": "npx esbuild --bundle ./src/index.ts --sourcemap --target=chrome58,firefox57,safari11,edge16,node12 --minify --outfile=dist.js",
    "_build-browser": "npx esbuild --bundle ./src/index.ts --sourcemap --target=chrome58,firefox57,safari11,edge16 --minify --outfile=browser.js",
    "_build-node12": "npx esbuild --bundle ./src/index.ts --platform=node --sourcemap --target=node12 --minify --outfile=node12.js",
    "pretest": "npx esbuild --bundle test/all.spec.ts --sourcemap --outfile=build/all.spec.js",
    "test": "jest build/all.spec.js --verbose",
    "prettify": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\""
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true
